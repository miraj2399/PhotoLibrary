<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="startWindow" prefHeight="400.0" prefWidth="600.0" stylesheets="/view/start.css" translateX="0.5" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.StartController">
   <children>
      <VBox alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" spacing="10.0" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="100.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Welcome to Photos!">
               <font>
                  <Font size="41.0" />
               </font>
            </Label>
            <TextField fx:id="username" alignment="CENTER" onAction="#handle_login" promptText="Username" />
            <Button fx:id="login" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#handle_login" text="Login" />
         </children>
      </VBox>
   </children>
</AnchorPane>
